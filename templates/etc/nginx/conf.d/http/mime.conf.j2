# {{ ansible_managed }}

# Additional configuration file - Mime types
#
# This is a harmonised configuration file created by merging, and then modifying, the Ubuntu and CentOS default
# configuration files.
#
# This file is included within the 'http' directive of the main Nginx configuration file '/etc/nginx/nginx.conf'.

include /etc/nginx/mime.types;
default_type application/octet-stream;
